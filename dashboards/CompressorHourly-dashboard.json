{
    "timeGrain": "hour",
    "cards": [
        {
            "id": "card-device-table",
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "last",
                        "attribute": "drvn_flow",
                        "id": "drvn_flow_id"
                    },
                    {
                        "aggregator": "last",
                        "attribute": "drvr_rpm",
                        "id": "drvr_rpm_id"
                    }
                ],
                "range": {
                    "type": "periodToDate",
                    "count": -1,
                    "interval": "hour"
                },
                "timeGrain": "hour",
                "groupBy": [
                    "deviceid"
                ]
            },
            "content": {
                "columns": [
                    {
                        "dataSourceId": "deviceid",
                        "label": "Asset id"
                    },
                    {
                        "dataSourceId": "drvn_flow_id",
                        "label": "drvn_flow"
                    },
                    {
                        "dataSourceId": "drvr_rpm_id",
                        "label": "drvr_rpm"
                    }
                ],
                "thresholds": [
                    {
                        "dataSourceId": "drvn_flow_id",
                        "comparison": "<",
                        "value": 1000,
                        "label": "Flow",
                        "icon": "Warning--alt--filled",
                        "color": "red",
                        "severity": 1,
                        "showSeverityLabel": false
                    },
                    {
                        "dataSourceId": "drvn_flow_id",
                        "comparison": ">",
                        "value": 999,
                        "label": "Flow",
                        "icon": "Checkmark--filled",
                        "color": "green",
                        "severity": 1,
                        "showSeverityLabel": false
                    },
                    {
                        "dataSourceId": "drvr_rpm_id",
                        "comparison": "<",
                        "value": 1000,
                        "label": "RPM",
                        "icon": "Warning--alt--filled",
                        "color": "red",
                        "severity": 2,
                        "showSeverityLabel": false
                    },
                    {
                        "dataSourceId": "drvr_rpm_id",
                        "comparison": ">",
                        "value": 999,
                        "label": "RPM",
                        "icon": "Checkmark--filled",
                        "color": "green",
                        "severity": 2,
                        "showSeverityLabel": false
                    }
                ]
            },
            "size": "LARGE",
            "title": "Train Table",
            "type": "TABLE"
        },
        {
            "id": "timeseries_utilbydevice_card",
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "mean",
                        "attribute": "run_percent_per_hour",
                        "id": "run_percent_per_hour_id"
                    }
                ],
                "range": {
                    "count": -24,
                    "interval": "hour"
                },
                "timeGrain": "hour",
                "groupBy": [
                    "deviceid"
                ]
            },
            "content": {
                "series": [
                    {
                        "dataSourceId": "run_percent_per_hour_id",
                        "dataFilter": {
                            "deviceid": "73002"
                        },
                        "label": "73002Device"
                    },
                    {
                        "dataSourceId": "run_percent_per_hour_id",
                        "dataFilter": {
                            "deviceid": "73001"
                        },
                        "label": "73001Device"
                    },
                    {
                        "dataSourceId": "run_percent_per_hour_id",
                        "dataFilter": {
                            "deviceid": "73002"
                        },
                        "label": "73002Device"
                    },
                    {
                        "dataSourceId": "run_percent_per_hour_id",
                        "dataFilter": {
                            "deviceid": "73003"
                        },
                        "label": "73003Device"
                    },
                    {
                        "dataSourceId": "run_percent_per_hour_id",
                        "dataFilter": {
                            "deviceid": "73004"
                        },
                        "label": "73004Device"
                    }
                ],
                "xLabel": "Time",
                "yLabel": "Percentage"
            },
            "size": "MEDIUM",
            "title": "Run Status - Utilization per Hour per device",
            "type": "TIMESERIES"
        },
        {
            "id": "timeseries_utilization_hour_card",
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "mean",
                        "attribute": "run_percent_per_hour",
                        "id": "run_percent_per_hour_id"
                    }
                ],
                "range": {
                    "count": -48,
                    "interval": "hour"
                },
                "timeGrain": "hour"
            },
            "content": {
                "series": [
                    {
                        "dataSourceId": "run_percent_per_hour_id",

{
    "timeGrain": "day",
    "cards": [
        {
            "content": {
                "alt": "Level 1 Monitoring",
                "zoomMax": 1,
                "hotspots": [
                    {
                        "color": "#FFF",
                        "content": {
                            "attributes": [
                                {
                                    "dataSourceId": "drvn_p1",
                                    "label": "Inlet Pressure",
                                    "unit": "PSI"
                                },
                                {
                                    "dataSourceId": "drvn_p2",
                                    "label": "Outlet Pressure",
                                    "unit": "PSI"
                                }
                            ]
                        },
                        "locations": [
                            {
                                "xDataSourceId": "compressor_in_x_factor",
                                "yDataSourceId": "compressor_in_y_factor",
                                "dataFilter": {
                                    "deviceid": "AssetID"
                                }
                            }
                        ],
                        "thresholds": [
                            {
                                "color": "red",
                                "comparison": ">=",
                                "dataSourceId": "drvn_p1",
                                "icon": "icon--close--solid",
                                "isHotspot": true,
                                "value": 15000
                            },
                            {
                                "color": "green",
                                "comparison": ">",
                                "dataSourceId": "drvn_p1",
                                "icon": "icon--close--solid",
                                "isHotspot": true,
                                "value": 10000
                            },
                            {
                                "color": "yellow",
                                "comparison": "<=",
                                "dataSourceId": "drvn_p1",
                                "icon": "icon--close--solid",
                                "isHotspot": true,
                                "value": 10000
                            },
                            {
                                "color": "red",
                                "comparison": ">=",
                                "dataSourceId": "drvn_p2",
                                "icon": "icon--close--solid",
                                "isHotspot": true,
                                "value": 15000
                            },
                            {
                                "color": "green",
                                "comparison": ">",
                                "dataSourceId": "drvn_p2",
                                "icon": "icon--close--solid",
                                "isHotspot": true,
                                "value": 10000
                            },
                            {
                                "color": "yellow",
                                "comparison": "<=",
                                "dataSourceId": "drvn_p2",
                                "icon": "icon--close--solid",
                                "isHotspot": true,
                                "value": 10000
                            }
                        ]
                    }
                ],
                "image": "compressor_flow_range2"
            },
            "dataSource": {
                "attributes": [
                    {
                        "attribute": "drvn_p1",
                        "id": "drvn_p1"
                    },
                    {
                        "attribute": "drvn_p2",
                        "id": "drvn_p2"
                    },
                    {
                        "attribute": "compressor_in_y_factor",
                        "id": "compressor_in_y_factor"
                    },
                    {
                        "attribute": "compressor_in_x_factor",
                        "id": "compressor_in_x_factor"
                    }
                ],
                "range": {
                    "count": -1,
                    "interval": "hour",
                    "type": "periodToDate"
                },
                "groupBy": [
                    "AssetID"
                ]
            },
            "id": "compressor_flow_range2",
            "size": "LARGE",
            "title": "Polytropic Head. (ft * lb-f? /  lb m) vs Flow  ",
            "type": "IMAGE"
        },
        {
            "id": "AnomalyAlertCardTable",
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "count",
                        "attribute": "multivariate_isolation_alert",
                        "id": "multivariate_isolation_alert"
                    }
                ],
                "groupBy": [
                    "deviceid",
                    "train",
                    "site",
                    "service",
                    "Path"
                ],
                "timeGrain": "day"
            },
            "content": {
                "columns": [
                    {
                        "dataSourceId": "deviceid",
                        "label": "ID"
                    },
                    {
                        "dataSourceId": "train",
                        "label": "Train"
                    },
                    {
                        "dataSourceId": "site",
                        "label": "Site"
                    },
                    {
                        "dataSourceId": "service",
                        "label": "Service"
                    },
                    {
                        "dataSourceId": "Path",
                        "label": "OSI PI Path"
                    },
                    {
                        "dataSourceId": "multivariate_isolation_alert",
                        "label": "Isolation Alert Count"
                    },
                    {
                        "dataSourceId": "timestamp",
                        "label": "Alert Time",
                        "type": "TIMESTAMP"
                    }
                ],
                "thresholds": [
                    {
                        "dataSourceId": "multivariate_isolation_alert",
                        "comparison": ">",
                        "value": 0,
                        "showOnContent": true,
                        "label": "Isolation Severity",
                        "severity": 3
                    },
                    {
                        "dataSourceId": "multivariate_isolation_alert",
                        "comparison": ">",
                        "value": 10,
                        "showOnContent": true,
                        "label": "Isolation Severity",
                        "severity": 2
                    },
                    {
                        "dataSourceId": "multivariate_isolation_alert",
                        "comparison": ">",
                        "value": 20,
                        "showOnContent": true,
                        "label": "Isolation Severity",
                        "severity": 1
                    }
                ]
            },
            "size": "LARGE",
            "title": "Anomaly Alert Notices",
            "type": "TABLE"
        },
        {
            "id": "timeseries_hoursperday_card",
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "sum",
                        "attribute": "run_hours_per_day",
                        "id": "run_hours_per_day_id"
                    }
                ],
                "range": {
                    "count": -30,
                    "interval": "day"
                },
                "timeGrain": "day"
            },
            "content": {
                "series": [
                    {
                        "dataSourceId": "run_hours_per_day_id",
                        "label": "Sum"
                    }
                ],
                "xLabel": "Time",
                "yLabel": "Hours"
            },
            "size": "MEDIUM",
            "title": "Run Hours per Day",
            "type": "TIMESERIES"
        },
        {
            "id": "timeseries_utilization_card",
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "mean",
                        "attribute": "run_percent_per_day",
                        "id": "run_percent_per_day_id"
                    }
                ],
                "range": {
                    "count": -30,
                    "interval": "day"
                },
                "timeGrain": "day"
            },
            "content": {
                "series": [
                    {
                        "dataSourceId": "run_percent_per_day_id",
                        "label": "Average"
